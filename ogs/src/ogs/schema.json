{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "ogs",
  "title": "AngularOGS Test Schema",
  "type": "object",
  "additionalProperties": false,
  "description": "Test custom template",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    }
  },
  "required": ["name"]
}
